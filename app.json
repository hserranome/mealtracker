{
  "expo": {
    "name": "MealTracker",
    "slug": "mealtracker",
    "version": "0.0.1",
    "scheme": "mealtracker",
    "plugins": [
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/images/app/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mealtracker.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app/adaptive-icon.png",
        "monochromeImage": "./assets/images/app/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mealtracker.app"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "09fe56c8-0753-47cc-8a4f-89cc09bcda76"
      }
    }
  }
}